{
  "name": "@kaokei/di",
  "version": "3.0.1",
  "type": "module",
  "description": "Tiny di library depends on typescript decorator.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.cjs",
  "jsdelivr": "./dist/index.cjs",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "homepage": "https://github.com/kaokei/di",
  "author": "kaokei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaokei/di/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaokei/di.git"
  },
  "keywords": [
    "IoC",
    "DI",
    "Dependency injection"
  ],
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "tsc && vite build",
    "clean": "rm -rf dist/*",
    "public": "npm publish --access public",
    "postpublish": "git push origin --follow-tags",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "standard-version -r",
    "release:first": "standard-version --first-release"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vue/test-utils": "^2.4.6",
    "fs-extra": "^11.3.0",
    "inversify": "^6.2.2",
    "jsdom": "^25.0.1",
    "reflect-metadata": "^0.2.2",
    "standard-version": "^9.5.0",
    "typescript": "~5.6.3",
    "vite": "^5.4.14",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.5",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.2"
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
